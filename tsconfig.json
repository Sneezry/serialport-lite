{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": false,
        "sourceMap": false,
        "outDir": "out",
        "lib": [
            "es6"
        ],
        "rootDir": "src"
    },
    "include": [
        "src/*.ts",
        "src/**/*.ts",
        "test/*.ts",
        "test/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}